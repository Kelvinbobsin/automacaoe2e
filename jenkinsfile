pipeline {
    agent any

    stages {
        stage('clonar repositorio') {
            steps {
                git branch: 'main', url: 'https://github.com/Kelvinbobsin/automacaoe2e.git'
            }
        }
        stage('instalar dependencias') {
            steps {
                sh 'npm install'
            }
        } 
        stage('Executar testes') {
            steps {
                sh 'npm run cy:run'
            }
        }
    }
}
